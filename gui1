-- Load Spawner
local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/DeltaGay/femboy/refs/heads/main/GardenSpawner.lua"))()

-- Create ScreenGui
local gui = Instance.new("ScreenGui", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
gui.Name = "CustomSpawnerGUI"
gui.ResetOnSpawn = false

-- Main Frame
local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 300, 0, 300)
frame.Position = UDim2.new(0.5, -150, 0.5, -150)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true

-- UICorner for rounded style
local corner = Instance.new("UICorner", frame)
corner.CornerRadius = UDim.new(0, 10)

-- Title
local title = Instance.new("TextLabel", frame)
title.Text = "ðŸŒ± Garden Spawner"
title.Size = UDim2.new(1, -30, 0, 30)
title.Position = UDim2.new(0, 10, 0, 5)
title.BackgroundTransparency = 1
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextSize = 18
title.Font = Enum.Font.GothamBold
title.TextXAlignment = Enum.TextXAlignment.Left

-- Minimize Button
local minimize = Instance.new("TextButton", frame)
minimize.Size = UDim2.new(0, 20, 0, 20)
minimize.Position = UDim2.new(1, -25, 0, 5)
minimize.Text = "-"
minimize.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
minimize.TextColor3 = Color3.fromRGB(255, 255, 255)
minimize.Font = Enum.Font.GothamBold
minimize.TextSize = 14
Instance.new("UICorner", minimize).CornerRadius = UDim.new(0, 4)

-- Content Holder
local content = Instance.new("Frame", frame)
content.Size = UDim2.new(1, -20, 1, -50)
content.Position = UDim2.new(0, 10, 0, 40)
content.BackgroundTransparency = 1

-- Dropdown Creator
local function createDropdown(parent, titleText, options, onSelect)
	local container = Instance.new("Frame", parent)
	container.Size = UDim2.new(1, 0, 0, 60)
	container.BackgroundTransparency = 1

	local label = Instance.new("TextLabel", container)
	label.Size = UDim2.new(1, 0, 0, 20)
	label.Text = titleText
	label.TextColor3 = Color3.fromRGB(200, 200, 200)
	label.TextSize = 14
	label.Font = Enum.Font.Gotham

	local dropdown = Instance.new("TextButton", container)
	dropdown.Size = UDim2.new(1, 0, 0, 25)
	dropdown.Position = UDim2.new(0, 0, 0, 25)
	dropdown.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
	dropdown.TextColor3 = Color3.fromRGB(255, 255, 255)
	dropdown.Font = Enum.Font.Gotham
	dropdown.TextSize = 14
	dropdown.Text = "Select..."

	local selectedValue = nil

	dropdown.MouseButton1Click:Connect(function()
		local list = Instance.new("ScrollingFrame", dropdown)
		list.Size = UDim2.new(1, 0, 0, 100)
		list.Position = UDim2.new(0, 0, 1, 0)
		list.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
		list.BorderSizePixel = 0
		list.CanvasSize = UDim2.new(0, 0, 0, #options * 25)
		list.ScrollBarThickness = 4

		for _, option in pairs(options) do
			local opt = Instance.new("TextButton", list)
			opt.Size = UDim2.new(1, 0, 0, 25)
			opt.BackgroundTransparency = 1
			opt.TextColor3 = Color3.fromRGB(255, 255, 255)
			opt.TextSize = 14
			opt.Font = Enum.Font.Gotham
			opt.Text = option

			opt.MouseButton1Click:Connect(function()
				selectedValue = option
				dropdown.Text = option
				list:Destroy()
				onSelect(option)
			end)
		end
	end)

	return container
end

-- PET SELECTOR
local selectedPet = nil
local petDropdown = createDropdown(content, "Pet", Spawner.GetPets(), function(p)
	selectedPet = p
end)
petDropdown.Position = UDim2.new(0, 0, 0, 0)

-- SEED SELECTOR
local selectedSeed = nil
local seedDropdown = createDropdown(content, "Seed", Spawner.GetSeeds(), function(s)
	selectedSeed = s
end)
seedDropdown.Position = UDim2.new(0, 0, 0, 70)

-- EGG INPUT
local selectedEgg = nil
local eggInput = Instance.new("TextBox", content)
eggInput.PlaceholderText = "Type Egg Name"
eggInput.Text = ""
eggInput.Position = UDim2.new(0, 0, 0, 140)
eggInput.Size = UDim2.new(1, 0, 0, 25)
eggInput.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
eggInput.TextColor3 = Color3.fromRGB(255, 255, 255)
eggInput.Font = Enum.Font.Gotham
eggInput.TextSize = 14
eggInput.ClearTextOnFocus = false
eggInput.FocusLost:Connect(function()
	selectedEgg = eggInput.Text
end)

-- Spawn Buttons
local function makeButton(yPos, text, callback)
	local btn = Instance.new("TextButton", content)
	btn.Size = UDim2.new(1, 0, 0, 30)
	btn.Position = UDim2.new(0, 0, 0, yPos)
	btn.Text = text
	btn.BackgroundColor3 = Color3.fromRGB(90, 90, 90)
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 14
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 5)
	btn.MouseButton1Click:Connect(callback)
end

makeButton(180, "Spawn Pet", function()
	if selectedPet then
		Spawner.SpawnPet(selectedPet, 1, 2)
	end
end)

makeButton(220, "Spawn Seed", function()
	if selectedSeed then
		Spawner.SpawnSeed(selectedSeed)
	end
end)

makeButton(260, "Spawn Egg", function()
	if selectedEgg and selectedEgg ~= "" then
		Spawner.SpawnEgg(selectedEgg)
	end
end)

-- Minimize toggle
local isMinimized = false
minimize.MouseButton1Click:Connect(function()
	isMinimized = not isMinimized
	content.Visible = not isMinimized
	if isMinimized then
		frame.Size = UDim2.new(0, 300, 0, 40)
	else
		frame.Size = UDim2.new(0, 300, 0, 300)
	end
end)
